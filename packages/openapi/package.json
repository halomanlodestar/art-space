{
	"name": "@art-space/openapi",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"fetch": "curl -X GET \"http://localhost:8000/api-json\" -H \"accept: application/json\" -o openapi.json",
		"openapi:generate": "npx openapi-generator-cli generate -i openapi.json -g typescript-axios -o ."
	},
	"exports": {
		".": "./src/index.ts",
		"./auth": "./src/clients/auth.ts",
		"./users": "./src/clients/users.ts",
		"./communitites": "./src/clients/communities.ts",
		"./posts": "./src/clients/posts.ts",
		"./likes": "./src/clients/likes.ts"
	},
	"dependencies": {
		"typescript": "latest"
	},
	"devDependencies": {
		"@nestjs/swagger": "^11.0.3",
		"@openapitools/openapi-generator-cli": "^2.18.4"
	}
}
